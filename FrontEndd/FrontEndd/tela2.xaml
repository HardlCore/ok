<UserControl x:Class="FrontEndd.tela2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"            
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FrontEndd"
             mc:Ignorable="d" Background="Cyan" Height="720" Width="1090">
    <Grid Background="Cyan">

        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            
        </Grid.RowDefinitions>
        <Grid Grid.Row="1" Grid.Column="1" Background="Gray" >
            <Grid.RowDefinitions Background="white">

                <RowDefinition Height="100*" />
                <RowDefinition Height="100*"/>
                <RowDefinition Height="100*" />
            </Grid.RowDefinitions>



            <TextBox TextWrapping="Wrap" Text="RESULTADOS" Width="363" FontSize="25" Grid.Column="1" Margin="0,0,0,0"  Grid.Row="1"/>
        </Grid>
       
        

    </Grid>
</UserControl>
